{
	"name": "react-redux-webpack",
	"version": "0.0.0",
	"description": "a react project base on webpack4.x",
	"main": "index.js",
	"keywords": [ "react", "redux", "webpack" ],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"clean": "rimraf www/*",
		"build": "webpack --progress --colors",
		"build-dev": "cross-env NODE_ENV=development node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.dev.js",
		"build-prod": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress --colors --config ./build/webpack.config.prod.js",
		"de": "webpack-dev-server --hot --devtool --progress --color",
		"dev": " cross-env NODE_ENV=development node node_modules/.bin/webpack-dev-server --progress --color --config ./build/webpack.config.dev.js ",
		"prod": " cross-env NODE_ENV=production node node_modules/.bin/webpack-dev-server --progress --color --config ./build/webpack.config.prod.js ",
		"start": "npm run clean && npm run build && npm run dev",
		"start-dev": "npm run clean && npm run build-dev && npm run dev",
		"start-prod": "npm run clean && npm run build-prod && npm run prod"
	},
	"author": "guangwei.bao",
	"license": "MIT",
	"devDependencies": {
		"babel-core": "6.26.0",
		"babel-loader": "7.1.3",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.3.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"copy-webpack-plugin": "^4.5.2",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"eslint-loader": "^2.1.0",
		"file-loader": "^2.0.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.2",
		"node-sass": "^4.9.3",
		"optimize-css-assets-webpack-plugin": "^4.0.0",
		"react": "^15.5.4",
		"react-redux": "^5.0.4",
		"react-router": "^4.3.1",
		"react-router-redux": "^5.0.0-alpha.6",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0",
		"sass-loader": "^7.1.0",
		"source-map-loader": "^0.2.4",
		"style-loader": "^0.23.0",
		"url-loader": "^1.1.1",
		"webpack": "^4.17.0",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.5",
		"webpack-merge": "^4.1.4"
	},
	"dependencies": {
		"font-awesome": "^4.7.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/niceboybao/react-javascript-seeds.git",
		"private": true
	}
}
